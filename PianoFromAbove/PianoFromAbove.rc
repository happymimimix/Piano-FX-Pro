        MENUITEM MenuText4,ID_PLAY_PLAYPAUSE
        MENUITEM MenuText5,ID_PLAY_STOP
        MENUITEM MenuText6,ID_PLAY_SKIPFWD
        MENUITEM MenuText7,ID_PLAY_SKIPBACK
        MENUITEM SEPARATOR
