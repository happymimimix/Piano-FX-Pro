        MENUITEM MenuText14,ID_PLAY_VOLUMEUP
        MENUITEM MenuText15,ID_PLAY_VOLUMEDOWN
        MENUITEM MenuText16,ID_PLAY_MUTE
    END
    POPUP MenuText17
    BEGIN
        MENUITEM MenuText18,ID_VIEW_CONTROLS,CHECKED
        MENUITEM MenuText19,ID_VIEW_KEYBOARD,CHECKED
