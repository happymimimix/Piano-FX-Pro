        MENUITEM MenuText9,ID_PLAY_DECREASERATE
        MENUITEM MenuText10,ID_PLAY_RESETRATE
        MENUITEM SEPARATOR
        MENUITEM MenuText11,ID_PLAY_NFASTER
        MENUITEM MenuText12,ID_PLAY_NSLOWER
